const express = require('express');
const app = express();


app.get('/', (req, res) => {
   $1
});

app.post('/', (req, res) => {
   $2
});

app.put('/:id', (req, res) => {
   $3
});

app.delete('/:id', (req, res) => {
   $4
});


module.exports = app
